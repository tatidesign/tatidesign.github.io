 var sizeRectangle = 50;
var positionRecatangle = 0;

var colorR = 0;

function setup(){

createCanvas(501,501);
background(100);

fill(200,80,20);

for(var y = 0; y < height; y = y + sizeRectangle){
for(var x = 0; x < width; x = x + sizeRectangle){

fill( 250-colorR, 250-colorR, 250-colorR);

rect(x,y,sizeRectangle,sizeRectangle);

colorR = colorR + 5;
sizeRectangle = sizeRectangle + 1;
}
}

}

